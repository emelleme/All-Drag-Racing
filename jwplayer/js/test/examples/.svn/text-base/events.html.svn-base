            	image: "http://content.bitsontherun.com/thumbs/gSzpo2wh-480.jpg",
            	mute: false,
				events:{
					onReady: function (){
						alert("ready");
					}
				}
            });
        </script>
        <h3>HTML code</h3>
    </body>
